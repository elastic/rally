{
  "template": "rally-*",
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "_default_": {
      "dynamic_templates": [
        {
          "strings": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "type": "string",
              "doc_values": true,
              "index": "not_analyzed"
            }
          }
        }
      ],
      "_all": {
        "enabled": false
      },
      "_source": {
        "enabled": true
      },
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "epoch_millis",
          "doc_values": true
        },
        "relative-time": {
          "type": "long",
          "doc_values": true
        },
        "trial-timestamp": {
          "type": "date",
          "format": "basic_date_time_no_millis",
          "doc_values": true
        },
        "environment": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "track": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "challenge": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "car": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "name": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "value": {
          "type": "float",
          "doc_values": true
        },
        "unit": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "sample-type": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "operation": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "operation-type": {
          "type": "string",
          "doc_values": true,
          "index": "not_analyzed"
        },
        "selected-challenge": {
          "type": "nested"
        }
      }
    }
  }
}
